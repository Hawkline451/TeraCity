                    },
                    {
                        "type" : "UIButton",
                        "id" : "textSearch",
                        "text" : "Text Search"
                    },
                    
                    {
                        "type" : "UIButton",
                        "id" : "close",
                        "text" : "Back"
                    }
                                        
