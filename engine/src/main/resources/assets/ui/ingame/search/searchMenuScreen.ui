{
    "type" : "engine:SearchMenuScreen",
    "skin" : "pauseMenu",
    "contents" : {
        "type" : "relativeLayout",
        "contents" : [
            {
                "type" : "columnLayout",
                "verticalSpacing" : 9,
                "contents" : [
                    {
                        "type" : "UILabel",
                        "text" : "SearchMenu"
                    },
                    {
                        "type" : "UISpace",
                        "size" : [1, 8]
                    },
                    {
                    	"type" : "UIText", 
                    	"id"   : "searchTextBox"
                    },
                    {               
                        "type" : "UIButton",
                        "id" : "searchButton",
